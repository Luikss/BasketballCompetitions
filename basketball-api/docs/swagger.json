                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "examples": {
                                    "List of games": {
                                        "value": [
                                            {
                                                "name": "Paide Cup"
                                            },
                                            {
                                                "name": "Veteranide Loivamine"
                                            }
                                        ]
                                    }
                                }
